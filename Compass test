import codewars_test as test
from solution import direction

@test.describe('Sample tests')
def tests():
    @test.it('Some examples')
    def tests():
        test.assert_equales(direction("S", 180), 'N')
        test.assert_equales(direction('SE', -45) 'E')
        test.assert_equales(direction('W', 495), 'NE')
